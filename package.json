{
  "name": "dokku-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build": "npm run build:server && next build",
    "start": "node server",
    "dev": "npm run build:server && node server",
    "start:mock-dokku": "npm run build:server && rm -f /tmp/dokku-daemon.sock && cd dokku && node mockSocketServer"
  },
  "resolutions": {
    "minipass": "2.7.0"
  },
  "dependencies": {
    "@emotion/cache": "^11.0.0",
    "@emotion/server": "^11.0.0",
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "clsx": "^1.1.1",
    "dockerode": "^3.2.1",
    "next": "10.0.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-topbar-progress-indicator": "^4.1.0",
    "sharp": "^0.26.3",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "@types/dockerode": "^3.2.1",
    "@types/node": "^14.14.10",
    "@types/react": "^16.14.2",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "**/*.js"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "overrides": [
      {
        "files": [
          "*.{ts,tsx}"
        ],
        "parser": "@typescript-eslint/parser",
        "plugins": [
          "@typescript-eslint"
        ],
        "extends": [
          "plugin:jsx-a11y/recommended",
          "plugin:react-hooks/recommended",
          "plugin:@typescript-eslint/recommended",
          "prettier/@typescript-eslint"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        }
      }
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.svg",
        "options": {
          "parser": "html"
        }
      }
    ]
  }
}
